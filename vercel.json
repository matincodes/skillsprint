{
    "version": 2,
    "builds": [
      {
        "src": "src/routes/(auth)/student/login.jsx",
        "use": "@vercel/node"
      },
      {
        "src": "src/routes/programmes/index.jsx",
        "use": "@vercel/node"
      },
      {
        "src": "src/**/*",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(auth)/student/login",
        "dest": "/src/routes/(auth)/student/login.jsx"
      },
      {
        "src": "/programmes",
        "dest": "/src/routes/programmes/index.jsx"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }