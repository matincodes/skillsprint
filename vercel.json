{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(auth)/student/login",
      "dest": "/src/routes/(auth)/student/login.jsx"
    },
    {
      "src": "/programmes",
      "dest": "/src/routes/programmes/index.jsx"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}